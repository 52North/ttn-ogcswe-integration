{
  "name": "ttn-ogcswe-integration",
  "version": "1.0.0",
  "description": "TheThingsNetwork integration with OGC SensorWeb",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "env $(cat .env | grep -v ^# | xargs) node --inspect dist/index.js",
    "build": "rimraf dist && tsc -p . || true",
    "build:watch": "rimraf dist && tsc -p . -w || true",
    "build:docker": "npm run build && docker build -t noerw/ttn-ogcswe-integration .",
    "dev:dist": "npm run build && npm run start:dev",
    "dev:src": "nodemon --ignore node_modules --ignore dist --exec npm run dev:dist",
    "lint": "tslint --fix '{{message-bridge,device-bridge,types}/**/,./}*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/noerw/ttn-ogcswe-integration.git"
  },
  "author": "Norwin Roosen <noerw@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noerw/ttn-ogcswe-integration/issues"
  },
  "homepage": "https://github.com/noerw/ttn-ogcswe-integration#readme",
  "devDependencies": {
    "@types/handlebars": "^4.0.34",
    "@types/js-yaml": "^3.9.0",
    "@types/node": "^8.0.7",
    "@types/request-promise-native": "^1.0.5",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.5.0",
    "tslint-consistent-codestyle": "^1.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "handlebars": "^4.0.10",
    "io-ts": "^0.6.0",
    "js-yaml": "^3.9.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "ttn": "^2.1.2"
  }
}
